*VT100.foreground: white
*VT100.background: black
*VT100.savelines: 4000

XTerm*bellIsUrgent: true 
! xterm*.font: -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1

! Emacs.font: DejaVu Sans Mono-12
! Emacs.menuBar: off
! Emacs.toolBar: -1
! Emacs.verticalScrollBars: off

mutt*Translations:    #override               <Key>BackSpace: string(0x7F)

URxvt*saveLines: 4000
URxvt*depth: 32
URxvt*foreground: rgba:ffff/ffff/ffff/ffff
URxvt*background: rgba:0000/0000/0000/eeee
URxvt*color12: rgba:0000/cccc/ffff/eeee
URxvt*scrollBar: false
URxvt*urgentOnBell: true
! URxvt*font: -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
URxvt*font: xft:Inconsolata:antialias=true:pixelsize=14:autohint=true
URxvt*loginShell: true
! URxvt*font: xft:SourceCodePro-Regular:pixelsize=10:autohint=true
! URxvt*boldFont: -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
! URxvt*termName: rxvt-256color

URxvt.perl-ext:      default,url-select
URxvt.keysym.M-u:    perl:url-select:select_next
URxvt.url-select.launcher:   /usr/bin/firefox
URxvt.url-select.underline: false

! Bind C-0, C-+ and C-= to activate medium, big and small font size respectively.
! URxvt.keysym.C-equal: command:\033]710;xft:proggytinysz:weight=medium\007
! URxvt.keysym.C-8:     command:\033]710;xft:SourceCodePro-Regular:pixelsize=20:autohint=true\007
! URxvt.keysym.C-9:     command:\033]710;xft:SourceCodePro-Regular:pixelsize=16:autohint=true\007
! URxvt.keysym.C-0:     command:\033]710;xft:SourceCodePro-Regular:pixelsize=14:autohint=true\007
! URxvt.keysym.C-minus: command:\033]710;xft:SourceCodePro-Regular:pixelsize=10:autohint=true\007
! URxvt.keysym.C-equal: command:\033]710;xft:SourceCodePro-Regular:pixelsize=8:autohint=true\007

URxvt.keysym.C-7:     command:\033]710;xft:Inconsolata:pixelsize=30:antialias=true:autohint=true\007
URxvt.keysym.C-8:     command:\033]710;xft:Inconsolata:pixelsize=24:antialias=true:autohint=true\007
URxvt.keysym.C-9:     command:\033]710;xft:Inconsolata:pixelsize=20:antialias=true:autohint=true\007
URxvt.keysym.C-0:     command:\033]710;xft:Inconsolata:pixelsize=16:antialias=true:autohint=true\007
URxvt.keysym.C-minus: command:\033]710;xft:Inconsolata:pixelsize=14:antialias=true:autohint=true\007
URxvt.keysym.C-equal: command:\033]710;xft:Inconsolata:pixelsize=12:antialias=true:autohint=true\007

! dzen2.font: -windows-proggytinysz-medium-r-normal--10-80-96-96-c-60-iso8859-1
! dzen2.font: xft:SourceCodePro-Regular:pixelsize=8:autohint=true

! xfig always special fonts
Fig.latexfonts: true 
Fig.specialtext: true

! Emacs.FontBackend: xft
! Emacs.font: Inconsolata-11
